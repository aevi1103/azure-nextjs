{
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["*.{png,jpg,gif,svg}", "*.css"]
  },
  "routes": [
    {
      "route": "/login",
      "rewrite": "/.auth/login/aad"
    },
    {
      "route": "/logout",
      "redirect": "/.auth/logout"
    },
    {
      "route": "/mes*",
      "allowedRoles": ["authenticated"]
    }
  ]
}
